<body>
    <!-- Header Starts -->
    <header class="main-header">

        <!-- Top Bar Starts -->
        <div class="top-bar hidden-xs">
            <div class="container">
                <ul class="list-unstyled list-inline clearfix">
                    <li class="pull-left">Optional Top Header Bar</li>
                    <li class="pull-right">
                        Email : <a href="mailto:info@newproject.com">info@newproject.com</a>
                    </li>
