            </div>
        </div>
        <!-- Top Bar Starts -->

        <!-- Navbar Starts -->
        <nav id="nav" class="navbar navbar-default navbar-static-top" role="navigation">
            <!-- Container Starts -->
            <div class="container">
                <!-- Navbar Header Starts -->
                <div class="navbar-header">
                    <!-- Collapse Button Starts -->
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- Collapse Button Ends -->

                    <!-- Logo Starts -->
                    <a href="@Url.Action("Index", "Home", new { area = "" })" class="navbar-brand">
                    <img src="@Url.Content("~/Content/images/logo_ph.png")" alt="Logo" style="width:22rem;"></a>
                    @*ToDo: Work this class in: class="img-responsive"*@
                    <!-- Logo Ends -->
                </div>
                <!-- Navbar Header Ends -->

                <!-- Navbar Collapse Starts -->
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("About", "About", "Home")</li>
                        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">DROPDOWN <span class="caret"></span></a>
                            <ul class="dropdown-menu" role="menu">
                                <li>@Html.ActionLink("SUBMENU1", "SubMenu1", "Home")</li>
                                <li>@Html.ActionLink("SUBMENU2", "SubMenu2", "Home")</li>
                                <li>@Html.ActionLink("SUBMENU3", "SubMenu3", "Home")</li>
                            </ul>
                        </li>
                        @if (Request.IsAuthenticated)
                        {
                            @*@Html.Encode(User.Identity.Name)*@
                            <li>@Html.ActionLink("SIGN OUT", "LogOff", "Account")</li>
                        }
                        else
                        {
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    SIGN IN
                                </a>
                                <ul class="dropdown-menu" role="menu">
                                    <li>@Html.ActionLink("SIGN IN", "Login", "Account", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
                                    <li>@Html.ActionLink("REGISTER", "Register", "Account", routeValues: null, htmlAttributes: new { id = "registerLink" })</li>
                                </ul>
                            </li>
                        }
                        @*<li>@Html.Partial("_LoginPartial")</li>*@
                    </ul>
                </div>
                <!-- Navbar Collapse Ends -->
            </div>
            <!-- Container Ends -->
        </nav>
